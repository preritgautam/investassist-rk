import React, { useMemo } from 'react';
import { PageComponent } from '../src/bootstrap/types';
import { getFpFloorPlan } from '../src/app/constant/FPBedsBaths';
import { FlexCol } from '../src/bootstrap/chakra/components/layouts/FlexCol';

const TestPage: PageComponent = () => {
  const result = useMemo(getResults, []);

  return (
    <FlexCol maxH="95vh">
      <b>Count: {result.length}</b>
      <pre>
        {JSON.stringify(result, null, 2)}
      </pre>
    </FlexCol>
  );
};

export default TestPage;


function getResults() {
  const results = [];
  Reflect.ownKeys(floorPlanConfiguration).forEach((fp: string) => {
    const floorPlan = getFpFloorPlan(fp);
    const noFP = floorPlan === null;
    const bedsMismatch = floorPlanConfiguration[fp].BD !== floorPlan?.beds &&
      !(floorPlanConfiguration[fp].BD === '0' && floorPlan?.beds === 'studio');
    const bathsMismatch = floorPlanConfiguration[fp].BA !== floorPlan?.baths && floorPlan?.beds !== 'studio';

    if (noFP || bedsMismatch || bathsMismatch) {
      results.push({
        fp,
        bd1: floorPlanConfiguration[fp].BD,
        bd2: floorPlan?.beds,
        ba1: floorPlanConfiguration[fp].BA,
        ba2: floorPlan?.baths,
      });
    }
  });
  return results;
}


